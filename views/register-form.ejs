<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include partials/head %>
    <title>Register</title>
  </head>
  <body>
    <%- include partials/header %>
    <form class="register-form" action="/users/register" method="POST" enctype="multipart/form-data">
      <h2 class="title-form">Register</h2>
      <div class="format-form">
        <label for="email">Email</label>
        <input class="style-input" type="email" name="email" id="email" placeholder="example@email.com">
        <label for="password">Password</label>
        <input class="style-input" type="password" name="password" id="password" placeholder="my password">
      </div>
      <input class="btn" type="button" value="Back">
      <button class="btn" type="submit">Send</button>
    </form>
    <script src="/javascripts/index.js"></script>
  </body>
</html>
